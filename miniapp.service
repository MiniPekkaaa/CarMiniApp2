[Unit]
Description=Mini App Gunicorn Service
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/c:/Projects — копия/MiniApp
Environment="PATH=/c:/Projects — копия/MiniApp/venv/bin"
ExecStart=/c:/Projects — копия/MiniApp/venv/bin/gunicorn -c gunicorn.conf.py app:app
Restart=always

[Install]
WantedBy=multi-user.target 